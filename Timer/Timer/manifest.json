 {
 "manifest_version": 3,
 "name": "DoomShot",
 "version": "1.0",
 "permissions": ["activeTab", "downloads"],
 "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
    ],
"action": {
    "default_icon": {
        "16": "img.png",
        "48": "img.png",
        "128": "img.png"
    },
    "default_popup": "popup.html",
    "default_title": "DoomShot"
    
}
}
